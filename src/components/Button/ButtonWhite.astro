---
const {buttonText, link} = Astro.props;

interface Props{
    buttonText:string,
    link: string
}
---

<a href={link} class="mt-6 px-6 py-3 border-2 border-black text-black hover:bg-black hover:text-white transition duration-300 uppercase tracking-wide" aria-label="Go Home">
    {buttonText}
</a>

